{
  "name": "@pv-hamed/configs",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Hamed Jafari <hamedjix@gmail.com>",
  "description": "Pointsville's ESLint configuration",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "pointsville",
    "eslint",
    "eslint-config",
    "react",
    "javascript",
    "typescript",
    "flat-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pv-hamed/configs.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "eslint .",
    "spell": "cspell ./* --no-progress",
    "test": "echo 0",
    "build": "unbuild",
    "postbuild": "./postbuild",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pre-commit": "npm run test && lint-staged && npm run spell"
  },
  "main": "./dist/index.js",
  "module": "./src/index.mjs",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./dist/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "1.50.0",
    "@eslint/compat": "1.3.1",
    "@next/eslint-plugin-next": "15.3.4",
    "@stylistic/eslint-plugin": "5.1.0",
    "deepmerge": "4.3.1",
    "eslint-plugin-better-tailwindcss": "3.4.3",
    "eslint-plugin-cypress": "5.1.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.20.0",
    "eslint-plugin-perfectionist": "4.13.0",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-vitest": "0.5.4",
    "globals": "16.3.0",
    "local-pkg": "1.1.1",
    "typescript-eslint": "8.35.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.30.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/eslint": "9.6.1",
    "cspell": "9.1.2",
    "eslint": "9.30.1",
    "husky": "9.1.7",
    "jest": "30.0.3",
    "lint-staged": "16.1.2",
    "pinst": "3.0.0",
    "prettier": "3.6.2",
    "semantic-release": "24.2.6",
    "typescript": "5.8.3",
    "unbuild": "3.5.0"
  },
  "peerDependencies": {
    "cypress": ">=8",
    "eslint": ">=9.22",
    "prettier": "3",
    "react": ">=16",
    "typescript": ">=4.7"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "cypress": {
      "optional": true
    },
    "jest": {
      "optional": true
    }
  }
}
